#include<iostream>
using namespace std;

int num01_main() {
	int k;
	cout << "몇 개의 정수를 입력합니까?";
	cin >> k;

	int* p = new int[k];

	for (int i = 0;i < k;i++) {
		int c;
		cout << "정수를 입력하시오 : ";
		cin >> c;
		p[i] = c;
	}

	cout << "입력된 정수는 : ";

	for (int i = 0 ;i < k;i++) {
		cout << p[i] << ", ";
	}
	delete[] p;
	return 0;
}
